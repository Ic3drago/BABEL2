{
  "functions": {
    "api/index.php": {
      "runtime": "vercel-php@0.7.3"
    }
  },
  "rewrites": [
    {
      "source": "/admin",
      "destination": "/public/admin.html"
    },
    {
      "source": "/pos",
      "destination": "/public/pos.html"
    },
    {
      "source": "/planilla",
      "destination": "/public/planilla.html"
    },
    {
      "source": "/api/(.*)",
      "destination": "/api/index.php?__vercel_path=/api/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/public/$1"
    }
  ]
}