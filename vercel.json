{
  "functions": {
    "api/index.php": {
      "runtime": "vercel-php@0.8.3"
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/index.php?__vercel_path=/api/$1"
    },
    {
      "source": "/pos",
      "destination": "/public/pos.html"
    },
    {
      "source": "/admin",
      "destination": "/public/admin.html"
    },
    {
      "source": "/planilla",
      "destination": "/public/planilla.html"
    },
    {
      "source": "/",
      "destination": "/public/index.html"
    }
  ]
}