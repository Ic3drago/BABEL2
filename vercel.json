{
  "functions": {
    "api/index.php": {
      "runtime": "vercel-php@0.7.3"
    }
  },
  "rewrites": [
    { "source": "/admin", "destination": "/admin.html" },
    { "source": "/pos", "destination": "/pos.html" },
    { "source": "/planilla", "destination": "/planilla.html" },
    {
      "source": "/api/(.*)",
      "destination": "/api/index.php?__vercel_path=/api/$1"
    }
  ]
}